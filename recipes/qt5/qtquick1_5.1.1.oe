## Recipe for building the qt5 module QtQuick1.
##
## QtQuick Qt5 Module.

LICENSE = "GFDL-1.3 & LGPL-2.1 & GPL-3.0"

inherit qt5module
require qt5-${PV}.inc
require qt5.inc

DEPENDS += "qtscript qtscript-dev qtsvg qtxmlpatterns"
DEPENDS_${PN} += "qtscript qtsvg qtxmlpatterns"
RDEPENDS_${PN} += "qtscript qtsvg qtxmlpatterns"
RDEPENDS_${PN}-dev += "qtscript-dev qtsvg-dev qtxmlpatterns-dev"