DESCRIPTION = "Setup script needed for the QT5 sdk"

RECIPE_TYPES = "canadian-cross"

SRC_URI += "file://setup.sh"

do_install () {
	cp ${SRCDIR}/setup.sh ${D}
}

QT5_TOOLS="host:qmake5-utilities"
QT5_TOOLS:HOST_OS_mingw32 = ""

PACKAGES = "${PN}"
RDEPENDS_${PN} += "${QT5_TOOLS}"
FILES_${PN} = "/setup.sh"
